<?php

namespace My\Model;

use Craft\Syn\Model;

class User
{

    use Model;

    /** @var int */
    public $id;

    /** @var string */
    public $username;

    /** @var string */
    public $password;

    /** @var string email */
    public $email;

    /** @var int */
    public $rank = 1;

} 